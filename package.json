{
  "name": "hangul-trie",
  "version": "0.1.1",
  "description": "한글을 지원하는 Compressed Trie 자료구조",
  "license": "MIT",
  "author": "Donghyeon Lee <ehdgus001928@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HENOZIWD/hangul-trie.git"
  },
  "keywords": [
    "한글",
    "한국어",
    "hangul",
    "Korean",
    "trie",
    "트라이",
    "자료구조",
    "자동완성",
    "autocomplete",
    "search",
    "검색"
  ],
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^25.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "es-hangul": "^2.3.8"
  }
}
